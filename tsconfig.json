{
	"include": ["src/**/*.ts"],
	"compilerOptions": {
		"lib": ["ES5", "ES6", "ES7"],
		"target": "ES2020",
		"module": "ESNext",
		"moduleResolution": "bundler",
        "allowSyntheticDefaultImports": true,
        "resolveJsonModule": true,
        "skipLibCheck": true,

		"noEmit": true,
		"strict": true
	}
}
